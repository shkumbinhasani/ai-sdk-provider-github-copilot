{
  "name": "ai-sdk-provider-github-copilot",
  "version": "0.0.1",
  "description": "Vercel AI SDK provider for GitHub Copilot - Use your Copilot subscription with the AI SDK",
  "keywords": [
    "ai-sdk",
    "github-copilot",
    "copilot",
    "openai",
    "language-model",
    "llm",
    "vercel-ai"
  ],
  "homepage": "https://github.com/shkumbinhasani/ai-sdk-provider-github-copilot",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shkumbinhasani/ai-sdk-provider-github-copilot.git"
  },
  "license": "MIT",
  "author": "Shkumbin Hasani",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run clean && npm run build",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/openai-compatible": "^2.0.2",
    "@ai-sdk/provider": "^2.0.0",
    "@ai-sdk/provider-utils": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.17.24",
    "ai": "^6.0.5",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "ai": "^6.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
